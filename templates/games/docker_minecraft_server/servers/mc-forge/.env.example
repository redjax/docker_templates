# MC_SERV#_DIFFICULTY=

############
# SERVER 1 #
############
## Get your Curse Forge API key at:
#    https://console.curseforge.com/#/api-keys
MC_SERVER_CURSE_FORGE_API_KEY=

## Default: mc-server_forge
MC_SERV_CONTAINER_NAME=
## Default: 25565
MC_SERV_PORT=25566
## Default: true
MC_SERV_TTY=
## Default: NEOFORGE
MC_SERV_TYPE=
## Default: survival, Options: creative, survival, adventure, spectator
MC_SERV_GAME_MODE=
## Default: Welcome to the Neoforge server!
MC_SERV_MOTD=
## Default: normal
MC_SERV_DIFFICULTY=
## Default: 4G
MC_SERV_MEMORY=8G
## Default: NA
#  This line is commented in the docker-compose.yml file by default.
#  To import a world, place it in ./worlds, then add the world's name here
MC_SERV_WORLD=
## Default: 20
MC_SERV_MAX_PLAYERS=
## Default: 10000
MC_SERV_MAX_WORLD_SIZE=
## Default: true
MC_SERV_ALLOW_NETHER=
## Default: true
MC_SERV_COMMAND_BLOCK=
## Default: 256
MC_SERV_MAX_BUILD_HEIGHT=
## Default: 60000
MC_SERV_MAX_TICK_TIME=
## Default: 10
MC_SERV_VIEW_DISTANCE=
## Default: true
MC_SERV_PVP=
## Default: true
MC_SERV_ONLINE_MODE=
## Default=false
MC_SERV_WHITELIST_ENABLE=

## Default: ./data
MC_SERV_DATA_DIR=
## Default: ./whitelist.json
MC_SERVER_WHITELIST_FILE=
## Default: mods/default.txt
MC_SERVER_MODS_FILE=
