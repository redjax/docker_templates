## This env var overrides default Gitlab configurations.
#  You can find all options here: https://gitlab.com/gitlab-org/omnibus-gitlab/blob/master/README.md
#
#  Options must all be on a single line; where you would use "double quotes", use 'single quotes' instead.
#  Each option must be separated by a semicolon and a space (; ).
GITLAB_OMNIBUS_CONFIG="gitlab_rails['gitlab_ssh_host'] = 'gitlab.domain2.com'; gitlab_rails['gitlab_shell_ssh_port'] = 2424; gitlab_rails['smtp_enable'] = false; gitlab_rails['smtp_address'] = 'smtp.example.com'; gitlab_rails['smtp_port'] = 587; gitlab_rails['smtp_user_name'] = 'youruser@domain.com'; gitlab_rails['smtp_password'] = 'your-password-or-app-token'; gitlab_rails['smtp_domain'] = 'smtp.example.com'; gitlab_rails['smtp_authentication'] = 'login'; gitlab_rails['smtp_enable_starttls_auto'] = true; gitlab_rails['smtp_tls'] = false; gitlab_rails['smtp_openssl_verify_mode'] = 'peer'; gitlab_rails['gitlab_email_from'] = 'gitlab@example.com'; gitlab_rails['gitlab_email_reply_to'] = 'no-reply@example.com'"
